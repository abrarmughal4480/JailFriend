{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"],
      "@/models/*": ["./models/*"],
      "@/config/*": ["./config/*"],
      "@/utils/*": ["./utils/*"],
      "@/controllers/*": ["./controllers/*"],
      "@/routes/*": ["./routes/*"],
      "@/middlewares/*": ["./middlewares/*"]
    }
